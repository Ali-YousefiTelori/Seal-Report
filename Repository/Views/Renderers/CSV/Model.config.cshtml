@using Seal.Model;
@{
    ReportViewTemplate Template = Model;

    //Parameters for this template
    Template.Parameters.Add(new Parameter() { Name = "csv_separator", DisplayName = "Separator", Description = "The separator used to generate the CSV file. If empty, the default value of the current culture is used." });
    Template.Parameters.Add(new Parameter() { Name = "use_display_format", DisplayName = "Use element format", BoolValue = true, Description = "If true, the display format of the element is applied to the values displayed in the CSV file" });
}
